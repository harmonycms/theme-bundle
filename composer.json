{
  "name": "harmony/theme-bundle",
  "description": "HarmonyCMS theme bundle",
  "homepage": "https://projectharmony.github.io",
  "type": "symfony-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "David Sanchez",
      "email": "david38sanchez@gmail.com",
      "homepage": "http://davidsanchez.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "composer/composer": "^1.6",
    "harmony/core-bundle": "^1.0-dev",
    "liip/theme-bundle": "dev-master",
    "symfony/console": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Harmony\\Bundle\\ThemeBundle\\": ""
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "auto-scripts": {
      "security-checker security:check": "script"
    }
  }
}
